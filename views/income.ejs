<div class="tpl-content-wrapper">
    <div class="row-content am-cf">
        <div class="row  am-cf">
            <div class="am-u-sm-12 am-u-md-12 am-u-lg-4">
                <div class="widget am-cf">
                    <div class="widget-head am-cf">
                        <% var date = new Date();%>
                        <div class="widget-title am-fl">月度财务收支计划</div>
                        <div class="widget-function am-fr">
                            <a href="javascript:;" class="am-icon-cog"></a>
                        </div>
                    </div>
                    <div class="widget-body am-fr">
                        <div class="am-fl">
                            <div class="widget-fluctuation-period-text">
                                ￥<%= monthOrderNum%>
                            </div>
                        </div>
                        <div class="am-fr am-cf">
                            <div class="widget-fluctuation-description-amount text-success" am-cf>
                                +￥<%= monthOrderProfit%>

                            </div>
                            <div class="widget-fluctuation-description-text am-text-right">
                                <%= date.getMonth() + 1%>月份收入
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="am-u-sm-12 am-u-md-6 am-u-lg-4">
                <div class="widget widget-purple am-cf">
                    <div class="widget-statistic-header">
                        <%= date.toLocaleDateString() %>（今日）财务收支
                    </div>
                    <div class="widget-statistic-body">
                        <div class="widget-statistic-value">
                            ￥<%= dayOrderNum%>
                        </div>
                        <div class="widget-statistic-description">
                            今日盈利 <strong><%= dayOrderProfit%>元</strong> 人民币
                        </div>
                        <span class="widget-statistic-icon am-icon-support"></span>
                    </div>
                </div>
            </div>
        </div>

        <div class="row am-cf">
            <div class="am-u-sm-12 am-u-md-8">
                <div class="widget am-cf">
                    <div class="widget-head am-cf">
                        <div class="widget-title am-fl">账单清算</div>
                        <div class="widget-function am-fr">
                            <a href="javascript:;" class="am-icon-cog"></a>
                        </div>
                    </div>
                    <div class="bill_container">
                        <h6 class="text-success">待清账额度：<strong>￥<%= billValue%></strong></h6>
                        <button type="button" class="am-btn am-btn-secondary bill_btn">
                        点击清账</button>
                    </div>
                    <!-- <div class="widget-body-md widget-body tpl-amendment-echarts am-fr" id="tpl-echarts">
                    </div> -->
                </div>
            </div>

        </div>
    </div>
</div>